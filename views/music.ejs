<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
    <style>
        body {
            background-color: #b5a789;
            font-family: Georgia, "Times New Roman", Times, serif;
            padding: 10px;
        }
        h1{
            text-align:center;
            font-size:25px;
        }
        ol{
            font-size:30px;
        }
        img{
            width:250px;
            height:250px;
        }
    </style>
</head>
<body>
    <% include nav/header.ejs %>
    <h1><%= title %></h1>
    <ol>
        <% jdata.music.forEach(function(item){%>
        <li>
            <%= item.Name%>-<%= item.artist%><br>
            <img src=<%= item.img%>>
            <a href=<%= item.link%>>link</a>
        </li>
        <%}); %>
    </ol>
</body>
</html>
